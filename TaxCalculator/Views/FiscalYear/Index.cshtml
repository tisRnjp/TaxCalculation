@model IEnumerable<TaxCalculator.Models.FiscalYear>

@{
    ViewBag.Title = "Fiscal Year";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Fiscal Year</h2>
<br />
<p>
    @Html.ActionLink("New Fiscal Year", "New", "FiscalYear", null, new { @class = "btn btn-primary" })
</p>
<br />
<table class="table" id="fiscalYears">
    <thead class="thead-light">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sequence)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sequence)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
        }
    </tbody>
</table>


@section scripts{
    <script>
        $(document).ready(function () {
            $("#fiscalYears").DataTable();




        });



    </script>


}