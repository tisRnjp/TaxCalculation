@model TaxCalculator.Models.FiscalYear
@{
    /**/

    /**/

    ViewBag.Title = "Fiscal Year";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Fiscal Year</h2>

@using (Html.BeginForm("Save", "FiscalYear"))
{
    @Html.ValidationSummary(true, "Please fix the following errors.")

    <div class="form-group row">
        @Html.LabelFor(m => m.FY, new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.FY, new { @class = "form-control" })
        </div>

    </div>

    <div class="form-group row">
        @Html.LabelFor(m => m.Sequence, new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.Sequence, new { @class = "form-control" })
        </div>

    </div>

    @Html.HiddenFor(m => m.Id)
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-primary"> Save</button>

}




@section scripts{
    @Scripts.Render("~/bundles/jqueryval")

}